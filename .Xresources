URxvt.intensityStyles:	false
!! URxvt.background:       [70]#000000
URxvt.depth:		32
!!URxvt.font: 		xft:UbuntuMono-R:size=16
URxvt.font: 		xft:FiraCode-Medium:size=15
!!URxvt.font: 		xft:DejaVuSansMono:size=15
URxvt.scrollBar:	false
URxvt.cursorColor:	white

!! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font,keyboard-select
URxvt.colorUL:		#4682B4
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
!! URxvt.resize-font.smaller:	C-Down
!! URxvt.resize-font.bigger:	C-Up
URxvt.letterSpace: -1
!! Matcher
URxvt.matcher.button:	1
URxvt.keysym.Shift-Delete: perl:matcher:list
!! copy-paste
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
!! borders
URxvt.borderWidth: 0
URxvt.internalborder: 0
URxvt.externalborder: 0
!! Anoying picture mode
URxvt.iso14755: false
URxvt.iso14755_52: false
!! Ctrl + arrows support
URxvt.keysym.Control-Up:     \033[1;5A
URxvt.keysym.Control-Down:   \033[1;5B
URxvt.keysym.Control-Left:   \033[1;5D
URxvt.keysym.Control-Right:  \033[1;5C
!! buffer for secondary screen
URxvt.secondaryScreen: 1
URxvt.secondaryScroll: 0
URxvt.secondaryWheel: 1
